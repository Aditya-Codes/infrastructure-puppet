# This file is in puppet. See modules:tac_asf/templates/local_settings.py.erb

DATABASE_ENGINE = 'postgresql_psycopg2'
DATABASE_NAME = 'tac_app' 
DATABASE_USER = '<%= @tac_dbuser %>'
DATABASE_PASSWORD = '<%= @tac_dbpasswd %>'
DATABASE_HOST = '<%= @tac_dbhost %>'
DATABASE_PORT = '<%= @tac_dbport %>'

# Debug should normally be False when in production
DEBUG = False
TEMPLATE_DEBUG = DEBUG

import admins
reload(admins)

ADMINS = admins.ADMINS
MANAGERS = ADMINS

EMAIL_HOST = "localhost"
